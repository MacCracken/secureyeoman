{
  "annotations": { "list": [] },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "id": null,
  "links": [],
  "panels": [
    {
      "title": "Tasks Overview",
      "type": "stat",
      "gridPos": { "h": 4, "w": 6, "x": 0, "y": 0 },
      "targets": [
        { "expr": "secureyeoman_tasks_total", "legendFormat": "Total Tasks" }
      ],
      "fieldConfig": {
        "defaults": { "thresholds": { "steps": [
          { "color": "green", "value": null },
          { "color": "yellow", "value": 100 },
          { "color": "red", "value": 1000 }
        ]}}
      }
    },
    {
      "title": "Task Queue Depth",
      "type": "gauge",
      "gridPos": { "h": 4, "w": 6, "x": 6, "y": 0 },
      "targets": [
        { "expr": "secureyeoman_tasks_queue_depth", "legendFormat": "Queue" }
      ],
      "fieldConfig": {
        "defaults": {
          "max": 200,
          "thresholds": { "steps": [
            { "color": "green", "value": null },
            { "color": "yellow", "value": 50 },
            { "color": "red", "value": 100 }
          ]}
        }
      }
    },
    {
      "title": "Success Rate",
      "type": "gauge",
      "gridPos": { "h": 4, "w": 6, "x": 12, "y": 0 },
      "targets": [
        { "expr": "secureyeoman_tasks_success_rate * 100", "legendFormat": "%" }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "max": 100,
          "thresholds": { "steps": [
            { "color": "red", "value": null },
            { "color": "yellow", "value": 80 },
            { "color": "green", "value": 95 }
          ]}
        }
      }
    },
    {
      "title": "API Latency (avg ms)",
      "type": "stat",
      "gridPos": { "h": 4, "w": 6, "x": 18, "y": 0 },
      "targets": [
        { "expr": "secureyeoman_api_latency_avg_ms", "legendFormat": "Latency" }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "thresholds": { "steps": [
            { "color": "green", "value": null },
            { "color": "yellow", "value": 200 },
            { "color": "red", "value": 500 }
          ]}
        }
      }
    },
    {
      "title": "Task Duration Percentiles",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 4 },
      "targets": [
        { "expr": "secureyeoman_tasks_duration_ms{quantile=\"0.5\"}", "legendFormat": "p50" },
        { "expr": "secureyeoman_tasks_duration_ms{quantile=\"0.95\"}", "legendFormat": "p95" },
        { "expr": "secureyeoman_tasks_duration_ms{quantile=\"0.99\"}", "legendFormat": "p99" }
      ],
      "fieldConfig": { "defaults": { "unit": "ms" } }
    },
    {
      "title": "Memory & CPU",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 4 },
      "targets": [
        { "expr": "secureyeoman_memory_used_mb", "legendFormat": "Memory (MB)" },
        { "expr": "secureyeoman_cpu_percent", "legendFormat": "CPU (%)" }
      ]
    },
    {
      "title": "API Calls & Errors",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 12 },
      "targets": [
        { "expr": "rate(secureyeoman_api_calls_total[5m])", "legendFormat": "Calls/s" },
        { "expr": "rate(secureyeoman_api_errors_total[5m])", "legendFormat": "Errors/s" }
      ]
    },
    {
      "title": "Security",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 12 },
      "targets": [
        { "expr": "rate(secureyeoman_auth_failures_total[5m])", "legendFormat": "Auth Failures/s" },
        { "expr": "rate(secureyeoman_rate_limit_hits_total[5m])", "legendFormat": "Rate Limits/s" },
        { "expr": "rate(secureyeoman_blocked_requests_total[5m])", "legendFormat": "Blocked/s" }
      ]
    },
    {
      "title": "Token Usage & Cost",
      "type": "stat",
      "gridPos": { "h": 4, "w": 12, "x": 0, "y": 20 },
      "targets": [
        { "expr": "secureyeoman_tokens_used_today", "legendFormat": "Tokens Today" },
        { "expr": "secureyeoman_cost_usd_today", "legendFormat": "Cost Today ($)" }
      ]
    },
    {
      "title": "Audit Chain",
      "type": "stat",
      "gridPos": { "h": 4, "w": 12, "x": 12, "y": 20 },
      "targets": [
        { "expr": "secureyeoman_audit_entries_total", "legendFormat": "Entries" },
        { "expr": "secureyeoman_audit_chain_valid", "legendFormat": "Valid" }
      ]
    }
  ],
  "schemaVersion": 39,
  "tags": ["secureyeoman", "agent"],
  "templating": { "list": [] },
  "time": { "from": "now-1h", "to": "now" },
  "timepicker": {},
  "timezone": "browser",
  "title": "SecureYeoman Dashboard",
  "uid": "secureyeoman-main",
  "version": 1
}
